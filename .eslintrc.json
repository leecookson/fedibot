{
  "root": true,
  "ignorePatterns": [
    "esbuild.js"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "browser": true,
    "commonjs": false,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "eslint no-useless-escape": 0
  }
}